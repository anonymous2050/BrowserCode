
{
    "manifest_version": 2,
    "name": "Phish+",
    "version": "1.0",
    "description": "Highlights potential phishing links on webpages.",
    "permissions": ["activeTab"],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "browser_action": {
        "default_icon": {
           
            "128": "icon.png"
        },
        "default_popup": "popup.html"
    },
    "icons": {
        "128": "icon.png"
    }
}
